from collections import Counter

numbershoes = int(input())

shoesize = input()
availshoes = list(shoesize.split(" "))

numberofcust = int(input())


shoedemand =dict([ map(str,input().split(' ')) for x in range(numberofcust)])
total = 0
for i in shoedemand.keys():
    for j in availshoes:
        if i == j:
            total += int(shoedemand[i])

        else:
            pass


print(total)
